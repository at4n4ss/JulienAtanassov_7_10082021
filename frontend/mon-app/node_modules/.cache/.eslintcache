julien\\frontend\\mon-app\\src\\components\\LoginForm.js",["109"],"// Imports\r\nimport React, { useState } from 'react';\r\nimport '../styles/style.css';\r\nimport Form from 'react-bootstrap/Form';\r\nimport Button from 'react-bootstrap/Button';\r\nimport { useHistory } from 'react-router-dom';\r\nimport axios from 'axios';\r\n\r\n// Fonction permettant de gérer le formulaire de connexion\r\nconst LoginForm = () => {\r\n  const history = useHistory();\r\n  const [emailLog, setEmailLog] = useState('');\r\n  const [passwordLog, setPasswordLog] = useState('');\r\n  let URL = 'http://localhost:3002/api/users/login';\r\n  let data = { email: emailLog, password: passwordLog };\r\n\r\n  const login = e => {\r\n    e.preventDefault();\r\n    axios(URL, {\r\n      method: 'POST',\r\n      headers: { 'content-type': 'application/json' },\r\n      data: data\r\n    })\r\n      .then(response => {\r\n        localStorage.setItem('userId', response.data.userId);\r\n        localStorage.setItem('token', 'bearer' + ' ' + response.data.token);\r\n        history.push('/articles');\r\n      })\r\n      .catch(error => {\r\n        console.log(error);\r\n      });\r\n  };\r\n  const redirect = () => {\r\n    history.push('/sinscrire');\r\n  };\r\n  return (\r\n    <Form>\r\n      <Form.Group className='mb-3' controlId='formBasicUsername'>\r\n        <Form.Label>Email</Form.Label>\r\n        <Form.Control\r\n          type='email'\r\n          onChange={e => {\r\n            setEmailLog(e.target.value);\r\n          }}\r\n          placeholder='exemple@gmail.com'\r\n        />\r\n      </Form.Group>\r\n      <Form.Group className='mb-3' controlId='formBasicPassword'>\r\n        <Form.Label>Password</Form.Label>\r\n        <Form.Control\r\n          type='password'\r\n          onChange={e => {\r\n            setPasswordLog(e.target.value);\r\n          }}\r\n          placeholder='Password'\r\n        />\r\n      </Form.Group>\r\n\r\n      <Button onClick={login} variant='primary' type='submit'>\r\n        Se connecter\r\n      </Button>\r\n      <Button onClick={redirect} className='registerBtn' variant='primary'>\r\n        S'inscrire\r\n      </Button>\r\n    </Form>\r\n  );\r\n};\r\n\r\nexport default LoginForm;\r\n","C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\displayContent.js",["110","111","112","113"],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\DisplayContent.js",["114"],"// Imports\r\nimport React, { Component } from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport '../styles/style.css';\r\nimport Card from 'react-bootstrap/Card';\r\nimport Container from 'react-bootstrap/Container';\r\nimport axios from 'axios';\r\n\r\n// Création de l'instance axios\r\nconst api = axios.create({\r\n  baseURL: 'http://localhost:3002/api/content/',\r\n  headers: {\r\n    common: {\r\n      Authorization: localStorage.getItem('token')\r\n    }\r\n  }\r\n});\r\n\r\n// Composant permettant d'afficher les articles\r\nclass DisplayContent extends Component {\r\n  state = {\r\n    contents: []\r\n  };\r\n  constructor(props) {\r\n    super(props);\r\n    this.getContents();\r\n  }\r\n\r\n  // Requête permettant de récupérer tous les articles\r\n  getContents = async () => {\r\n    let data = await api.get('/').then(({ data }) => data);\r\n    this.setState({ contents: data });\r\n  };\r\n\r\n  render() {\r\n    return (\r\n      <div>\r\n        {this.state.contents.map(content => (\r\n          <div className='containerArticles' key={content.id}>\r\n            <Card className='text-center'>\r\n              <Card.Header>{content.User.username}</Card.Header>\r\n              <Card.Body>\r\n                <Card.Title>{content.title}</Card.Title>\r\n                <Card.Text>{content.content}</Card.Text>\r\n                <Link to={'/article/' + content.id}>Accéder au topic</Link>\r\n              </Card.Body>\r\n            </Card>\r\n          </div>\r\n        ))}\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default DisplayContent;\r\n","C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\pages\\Profile.js",[],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\DisplayUser.js",[],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\CreateArticle.js",[],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\DisplayUserContents.js",[],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\pages\\Article.js",[],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\DisplayArticle.js",[],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\CreateComment.js",[],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\DisplayComments.js",[],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\DisplayUserComments.js",[],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\DeleteUser.js",["115"],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\pages\\Admin.js",[],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\AdminContents.js",[],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\AdminComments.js",[],"C:\\Openclassroom\\P7_atanassov_julien\\frontend\\mon-app\\src\\components\\IsConnected.js",["116","117"],{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","severity":1,"message":"123","line":5,"column":8,"nodeType":"124","messageId":"125","endLine":5,"endColumn":18},{"ruleId":"126","severity":1,"message":"127","line":26,"column":48,"nodeType":"128","messageId":"129","endLine":26,"endColumn":49},{"ruleId":"122","severity":1,"message":"130","line":1,"column":17,"nodeType":"124","messageId":"125","endLine":1,"endColumn":25},{"ruleId":"122","severity":1,"message":"131","line":6,"column":8,"nodeType":"124","messageId":"125","endLine"